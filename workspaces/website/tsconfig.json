{
  "extends": "astro/tsconfigs/base",
  "include": [
    "src",
    "index.d.ts"
  ],
  "compilerOptions": {
    "strictNullChecks": true,
    "allowJs": true,
    "declaration": true,
    "declarationDir": "./dist",
    "module": "ES2022",
    "outDir": "./dist",
    "target": "ES2021",
    "baseUrl": ".",
    "paths": {
      "~*": [
        "src/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "types": [
      "astro/client",
    ]
  }
}